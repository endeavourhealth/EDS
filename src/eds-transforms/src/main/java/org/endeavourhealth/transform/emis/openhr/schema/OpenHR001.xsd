<xs:schema xmlns="http://www.e-mis.com/emisopen" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.e-mis.com/emisopen" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.1.1">
	<!--Author: Jonathan Homer (jonathan.homer@e-mis.com)-->
	<!--Version: 2.1.4-->
	<xs:include schemaLocation="datatypes.xsd"/>
	<xs:include schemaLocation="vocabulary.xsd"/>
	<xs:element name="openHealthRecord" type="OpenHR001.OpenHealthRecord"/>
	<xs:complexType name="OpenHR001.OpenHealthRecord">
		<xs:sequence>
			<xs:element name="id" type="dt.uid">
				<xs:annotation>
					<xs:documentation>Message unique identifier</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="creationTime" type="dt.DateTime">
				<xs:annotation>
					<xs:documentation>Date and time of message generation on originating system</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="author" type="OpenHR001.MessageAuthor">
				<xs:annotation>
					<xs:documentation>Author of this message</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contentSpecification" type="OpenHR001.ContentSpecification">
				<xs:annotation>
					<xs:documentation>Specifies the constraints applied to the content of this message</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="requestMessage" type="dt.uid" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provides a reference to the unique identifier of the requesting message.  Deprecated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="adminDomain" type="OpenHR001.AdminDomain" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Administrative information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="healthDomain" type="OpenHR001.HealthDomain" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Health and Clinical Management information</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemaVersion" type="dt.Version" use="optional" default="1.3.0"/>
	</xs:complexType>
	<xs:complexType name="OpenHR001.MessageAuthor">
		<xs:sequence>
			<xs:element name="system" type="OpenHR001.AuthorSystem">
				<xs:annotation>
					<xs:documentation>Name and Version of the system</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="organisation" type="dt.uid" minOccurs="0"/>
			<xs:element name="SDSIdentifier" type="xs:token" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A valid SDS identifier that uniquely identifies the Organisation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nationalCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Example: National GP Practice Code</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="displayName" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OpenHR001.AuthorSystem">
		<xs:sequence>
			<xs:element name="systemType" type="voc.MessageAuthorSystemType"/>
			<xs:element name="displayName" type="xs:string" minOccurs="0"/>
			<xs:element name="version" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OpenHR001.AdminDomain">
		<xs:sequence>
			<xs:element name="person" type="OpenHR001.Person" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Demographic information for Patients and Users</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="patient" type="OpenHR001.Patient" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Information defining patients</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="organisation" type="OpenHR001.Organisation" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Information describing organisations</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="location" type="OpenHR001.Location" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Detailed information about physical locations related to organisations</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="user" type="OpenHR001.User" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Information relating to users</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="role" type="OpenHR001.Role" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Information relating to job roles at an organisation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="userInRole" type="OpenHR001.UserInRole" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Information relating to a user's relatiionship with an organisation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="localMixture" type="OpenHR001.LocalMixture" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Information defining local mixtures of drugs</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="auditTrail" type="OpenHR001.AuditTrail" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Deprecated audit trail information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="confidentialityPolicy" type="OpenHR001.ConfidentialityPolicy" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Information relating to confidentiality policies</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OpenHR001.HealthDomain">
		<xs:sequence>
			<xs:element name="encounter" type="OpenHR001.Encounter" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Details of an encouter between the auther and patient. An encouter contains a collection of other events</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="event" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The patient record components. Each component represents a discrete transaction between an author and the patient record</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="OpenHR001.Event"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="problem" type="OpenHR001.Problem" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="document" type="OpenHR001.Document" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="task" type="OpenHR001.PatientTask" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OpenHR001.Person">
		<xs:complexContent>
			<xs:extension base="dt.dbo">
				<xs:sequence>
					<xs:element name="id" type="dt.uid"/>
					<xs:element name="birthDate" type="dt.Date" minOccurs="0"/>
					<xs:element name="sex" type="voc.Sex" minOccurs="0"/>
					<xs:element name="forenames" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="108"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="callingName" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="35"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="surname" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="birthSurname" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="previousSurname" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="title" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="50"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="address" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Address information for the person.  No more than one of each type is permitted</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:complexContent>
								<xs:extension base="dt.Address">
									<xs:attribute name="updateMode" type="voc.UpdateMode" use="optional" default="none"/>
									<xs:attribute name="auditDeleteDate" type="dt.DateTime" use="optional"/>
									<xs:attribute name="auditDeleteUserInRole" type="dt.uid" use="optional"/>
									<xs:attribute name="auditDeleteInfo" type="xs:string" use="optional"/>
								</xs:extension>
							</xs:complexContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="contact" type="dt.Contact" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Contact information for the person.  No more than one of each type is permitted</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="maritalStatus" type="dt.Code" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Deprecated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ethnicGroup" type="dt.Code" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Deprecated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="religion" type="dt.Code" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Deprecated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="nationality" type="dt.Code" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Deprecated</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OpenHR001.Patient">
		<xs:complexContent>
			<xs:extension base="dt.dbo">
				<xs:sequence>
					<xs:element name="id" type="dt.uid"/>
					<xs:element name="patientIdentifier" type="dt.PatientIdentifier" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="patientPerson" type="dt.uid" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Links back to an entry in the person list</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="registeredGPUserInRole" type="dt.uid" minOccurs="0"/>
					<xs:element name="previousGPName" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Deprecated</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="150"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="usualGPUserInRole" type="dt.uid" minOccurs="0"/>
					<xs:element name="dateOfEntryCountry" type="dt.Date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Deprecated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="dateOfEntryArea" type="dt.Date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Deprecated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="dateOfDeath" type="dt.Date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The date of a patient's death.  Should be used in conjunction with an appropriate registration status in caseloadPatient/caseloadPatientHistory</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="dead" type="dt.bool" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Deprecated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="archived" type="dt.bool" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Indicates that the patient should not appear in search results.  Only applicable to deceased/inactive patients</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="confidentialityPolicy" type="dt.uid" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An optional confidentiality policy that should apply to viewing of any part of the patient's record</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseloadPatient" type="OpenHR001.CaseloadPatient" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Indicates the current patient status at the organisation</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseloadPatientHistory" type="OpenHR001.CaseloadPatient" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Indicates historical patient status entries</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="consent" type="dt.ConsentValue" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Deprecated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PatientCarerDetails" type="OpenHR001.PatientCarer" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Details of the patient's carer(s).  Not to be used to indicate patient contacts e.g. family members unless in a caring role</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PatientContact" type="OpenHR001.PatientContact" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OpenHR001.CaseloadPatient">
		<xs:complexContent>
			<xs:extension base="dt.dbo">
				<xs:sequence>
					<xs:element name="id" type="dt.uid" minOccurs="0"/>
					<xs:element name="organisation" type="dt.uid"/>
					<xs:element name="dateRecorded" type="dt.DateTime"/>
					<xs:element name="dateEnded" type="dt.DateTime" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Deprecated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="patientStatus" type="OpenHR001.PatientStatus"/>
					<xs:element name="patientType">
						<xs:complexType>
							<xs:simpleContent>
								<xs:extension base="voc.PatientType">
									<xs:attribute name="dummy" type="dt.bool" use="optional" fixed="false">
										<xs:annotation>
											<xs:documentation>For 'Other' patients, indicates that the patient type denotes a ficticious/test patient</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="userDefined" type="dt.bool" use="optional" fixed="false">
										<xs:annotation>
											<xs:documentation>For 'Other' patients, Indicates that the patient type is a locally defined type</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="originalTerm" type="xs:string" use="optional">
										<xs:annotation>
											<xs:documentation>For 'Other' patients, this is the description of the patient type on the originating system</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:extension>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="property" type="OpenHR001.CaseloadPatientProperty" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="customRegistrationField" type="OpenHR001.CustomRegistration" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="emisPatientNumber" type="xs:int" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The patient number on the originating system - not necessarily just an 'EMIS' patient number.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="preferredHCPUserInRole" type="dt.uid" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OpenHR001.CaseloadPatientProperty">
		<xs:complexContent>
			<xs:extension base="dt.dbo">
				<xs:sequence>
					<xs:element name="name" type="voc.CaseloadPatientProperty"/>
					<xs:element name="value">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="255"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OpenHR001.Organisation">
		<xs:complexContent>
			<xs:extension base="dt.dbo">
				<xs:sequence>
					<xs:element name="id" type="dt.uid"/>
					<xs:element name="name" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="organisationType" type="dt.Code" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Uncoded description of the organisation type</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="speciality" minOccurs="0">
						<xs:complexType>
							<xs:complexContent>
								<xs:extension base="dt.Code">
									<xs:attribute name="nationalCode" type="xs:int" use="optional"/>
									<xs:attribute name="specialityAbbreviation" use="optional">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="10"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:extension>
							</xs:complexContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="cdb" type="xs:int" minOccurs="0">
						<xs:annotation>
							<xs:documentation>For EMIS practices, the customer database number (CDB)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="nationalPracticeCode" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="20"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="mainLocation" type="dt.uid" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Primary location associated with the organisation</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="locations" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>All locations associated with the organisation.  This list must include the mainLocation</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:complexContent>
								<xs:extension base="dt.dbo">
									<xs:sequence>
										<xs:element name="location" type="dt.uid"/>
									</xs:sequence>
								</xs:extension>
							</xs:complexContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="caseload" type="OpenHR001.Caseload" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Deprecated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="parentOrganisation" type="dt.uid" minOccurs="0"/>
					<xs:element name="openDate" type="dt.DateTime" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The date the organisation became active</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="closeDate" type="dt.DateTime" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The date the organisation closed</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OpenHR001.Location">
		<xs:complexContent>
			<xs:extension base="dt.dbo">
				<xs:sequence>
					<xs:element name="id" type="dt.uid"/>
					<xs:element name="name" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="locationType" type="OpenHR001.LocationType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Uncoded description of the location type e.g. Hospital or GP Surgery</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="parentLocation" type="dt.uid" minOccurs="0"/>
					<xs:element name="mainContactPerson" type="dt.uid" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Deprecated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="notes" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="255"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="openDate" type="dt.DateTime" minOccurs="0"/>
					<xs:element name="closeDate" type="dt.DateTime" minOccurs="0"/>
					<xs:element name="address" type="dt.Address" minOccurs="0"/>
					<xs:element name="contact" type="dt.Contact" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Contact information for the location.  No more than one of each contact type is supported, and only 'W' and 'EM' are supported in this context</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OpenHR001.LocationType">
		<xs:complexContent>
			<xs:extension base="dt.Code"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OpenHR001.Caseload">
		<xs:complexContent>
			<xs:extension base="dt.dbo">
				<xs:sequence>
					<xs:element name="id" type="dt.uid"/>
					<xs:element name="title">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="referredOnly" type="dt.bool"/>
					<xs:element name="includeReferrals" type="dt.bool"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
