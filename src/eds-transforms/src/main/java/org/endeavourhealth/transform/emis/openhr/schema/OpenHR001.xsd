					<xs:choice minOccurs="0">
						<xs:element name="observation" type="OpenHR001.Observation"/>
						<xs:element name="diary" type="OpenHR001.Diary"/>
						<xs:element name="referral" type="OpenHR001.Referral"/>
						<xs:element name="medication" type="OpenHR001.Medication"/>
						<xs:element name="medicationIssue" type="OpenHR001.MedicationIssue"/>
						<xs:element name="report" type="OpenHR001.Report"/>
						<xs:element name="orderHeader" type="OpenHR001.OrderHeader">
							<xs:annotation>
								<xs:documentation>Test Request details, for EMIS internal use only</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="alert" type="OpenHR001.Alert"/>
						<xs:element name="allergy" type="OpenHR001.Allergy">
							<xs:annotation>
								<xs:documentation>Deprecated</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:element name="annotations" type="OpenHR001.Annotations" minOccurs="0">
						<xs:annotation>
