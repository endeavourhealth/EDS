							<xs:complexContent>
								<xs:extension base="OpenHR001.AssociatedText"/>
							</xs:complexContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="organisation" type="dt.uid" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="confidentialityPolicy" type="dt.uid" minOccurs="0"/>
