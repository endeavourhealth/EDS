						<xs:annotation>
							<xs:documentation>The clinically relevant date/time of the event</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="availabilityTimeStamp" type="dt.DateTime" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The date and time that the event was entered onto the clinical system</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="authorisingUserInRole" type="dt.uid" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The person clinically responsible for the event</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="enteredByUserInRole" type="dt.uid" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The person who entered the event onto the clinical system</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="code" type="dt.CodeQualified" minOccurs="0">
						<xs:annotation>
