	<xs:complexType name="IdentType">
		<xs:annotation>
			<xs:documentation>Base ID/Ref Type . DBID is the internal EMIS db, RefID references something else within the message that also has the same ID , GUID is used for GUID projects </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DBID" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used internally, ignored in any imported message</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RefID" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used internally for referncing components within the message. Unique to any component of a certtain type. Must be populated in any inbound message.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GUID" type="xs:string" minOccurs="0"/>
			<xs:element name="FileStatus" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The intended filing status of this item 0 = New,1 = edit, 2= delete 3 = reference only</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
