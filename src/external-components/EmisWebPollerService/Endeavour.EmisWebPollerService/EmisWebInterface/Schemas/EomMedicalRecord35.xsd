<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.e-mis.com/emisopen/MedicalRecord" targetNamespace="http://www.e-mis.com/emisopen/MedicalRecord" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:complexType name="IssueType">
		<xs:complexContent>
			<xs:extension base="MedicationType">
				<xs:sequence>
					<xs:element name="EstimatedCost" type="xs:double" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Estimated price at the time of issue</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Comment" type="xs:string" minOccurs="0"/>
					<xs:element name="IssuerID" type="IdentType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Person who authorised this particular issue</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DateTimePrint" type="xs:string" minOccurs="0"/>
					<xs:element name="DateTimeDispense" type="xs:string" minOccurs="0"/>
					<xs:element name="DispenserID" type="IdentType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>person who dispensed the drug</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BatchNumber" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ControlledDrugInfoType">
		<xs:sequence>
			<xs:element name="DduBatch" type="xs:string" minOccurs="0"/>
			<xs:element name="DduPrinted" type="xs:string" minOccurs="0"/>
			<xs:element name="DduCollect" type="xs:string" minOccurs="0"/>
			<xs:element name="DduNextDate" type="xs:string" minOccurs="0"/>
			<xs:element name="DduReduceBy" type="xs:string" minOccurs="0"/>
			<xs:element name="DduAfter" type="xs:string" minOccurs="0"/>
			<xs:element name="DduUntil" type="xs:string" minOccurs="0"/>
			<xs:element name="DduNextDose" type="xs:string" minOccurs="0"/>
			<xs:element name="DduOffset" type="xs:string" minOccurs="0"/>
			<xs:element name="DduClWorker" type="xs:string" minOccurs="0"/>
			<xs:element name="DduPickUp" type="xs:string" minOccurs="0"/>
			<xs:element name="DduDoseAbbrev" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DrugDeliveryType">
		<xs:sequence>
			<xs:element name="DoseQuantity" type="xs:float" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number of items per dose (e.g. 2)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DoseUnits" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>describes the dose units e,.g. tablets</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Frequency" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>frequency of ingesttion per day</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FrequencyUnits" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>frequency denominator (e.g. day)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DailyDose" type="xs:float" minOccurs="0">
				<xs:annotation>
					<xs:documentation>total daily dosage (decision support)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DailyDoseUnits" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MixtureItemType">
		<xs:complexContent>
			<xs:extension base="IdentType">
				<xs:sequence>
					<xs:element name="PreparationID" type="IntegerCodeType" minOccurs="0"/>
					<xs:element name="StrengthQuantity" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MedicationType">
		<xs:annotation>
			<xs:documentation>A drug within a record, used for both drug records and issue records</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="IdentType">
				<xs:sequence>
					<xs:element name="AuthorisedUserID" type="IdentType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The Person who has authorised the course - points to the person directory</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OriginalAuthor" type="AuthorType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The user who added the drug</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AssignedDate" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Start Date of the course (drug added)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DateLastIssue" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A record of the date the drug was last issued</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IssueMethod" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Method by which the drug is issued (H= hand Q= without script X= Private O = Outside, P = Print, S=Stored)</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="S"/>
								<xs:enumeration value="Q"/>
								<xs:enumeration value="H"/>
								<xs:enumeration value="X"/>
								<xs:enumeration value="O"/>
								<xs:enumeration value="P"/>
								<xs:enumeration value="N"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="DateRxExpire" type="xs:string" minOccurs="0"/>
					<xs:element name="DoseAbbreviation" type="xs:string" minOccurs="0"/>
					<xs:element name="Dosage" type="xs:string" minOccurs="0"/>
					<xs:element name="Quantity" type="xs:float" minOccurs="0"/>
					<xs:element name="QuantityUnits" type="xs:string" minOccurs="0"/>
					<xs:element name="QuantityRepresentation" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Full textual representation of the quantity</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Duration" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Duration of one issue</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MixtureIndicator" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Whether or not this is a mixture</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Drug">
						<xs:complexType>
							<xs:choice>
								<xs:element name="PreparationID" type="IntegerCodeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>The drug code (EMIS PREPARATION ID)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Mixture" type="MixtureType"/>
							</xs:choice>
						</xs:complexType>
					</xs:element>
					<xs:element name="PrescriptionType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>ACUTE , REPEAT or AUTOMATIC
</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="ACUTE"/>
								<xs:enumeration value="REPEAT"/>
								<xs:enumeration value="AUTOMATIC"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="DrugSource" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Textual description of what influenced the adding of a drug (e.g. Practice or Hospital)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RxReviewDate" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Individual Item Review Date</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PharmacyText" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Instructions to pharmacist</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RxText" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Any Text associated with the drug</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AuthorisedIssue" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The number of issues authorised before a review</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IssueCount" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The number issued for the course</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Status" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Whether currently authorised course (C) or a past Course (P)
</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="C"/>
								<xs:enumeration value="N"/>
								<xs:enumeration value="P"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="Advice" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Advice (e.g non drug advice- prodigy)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Template" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>prodigy guideline id</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TemplateComponent" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Prodigy (or template) component identifier</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TemplateInstance" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Instance idenifier of the guideline or template used</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DrugDelivery" type="DrugDeliveryType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Dosage information for decision support</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ControlledDrug" type="ControlledDrugInfoType" minOccurs="0"/>
					<xs:element name="ProblemLinkList" type="LinkListType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ItemBaseType">
		<xs:annotation>
			<xs:documentation>Base Item Type . Used  as a base class for coded entries of various kinds</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="IdentType">
				<xs:sequence>
					<xs:element name="AssignedDate" type="xs:string">
						<xs:annotation>
							<xs:documentation>The date to which the entry relates (not the date the entry was made)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DatePart" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Assigned date format 0=dd/mm/yyyy; 1=mmmm/yyyy; 2=yyyy; 3=Not Known</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:short">
								<xs:enumeration value="0"/>
								<xs:enumeration value="1"/>
								<xs:enumeration value="2"/>
								<xs:enumeration value="3"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="AssignedTime" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Time associated with the event.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Sequence" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Optional sequence number where date and time of a particular code are  identical</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AuthorID" type="IdentType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>References a user in the Person directory</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OriginalAuthor" type="AuthorType" minOccurs="0"/>
					<xs:element name="ExternalConsultant" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Describes any external consultant not on the system</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DisplayTerm" type="xs:string" minOccurs="0"/>
					<xs:element name="DescriptiveText" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Any additional text that the user may enter</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DataSource" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Type of data source. All 0 at this stage</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PolicyID" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Link to the confidentiality policy record, which describes who can view this record</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CodedItemBaseType">
		<xs:annotation>
			<xs:documentation>Base Coded Item Type. Extends the base item type with a code and term ID</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ItemBaseType">
				<xs:sequence>
					<xs:element name="Code" type="StringCodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The coded entry itself</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TermID" type="StringCodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>SNOMED, READ to equivalent TERMID structure. If there is no term ID then use code instead (i.e. code element=Termid element)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="QualifierList" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A list of qualifiers that alter the meaniing of the code</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Qualifier" type="QualifierType" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Problem" type="ProblemType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>If this item is a problem definition (first or new only- NOT reviews which are linked events)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ProblemLinkList" type="LinkListType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Problems associated with the item</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ReferralLinkList" type="LinkListType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="QualifierType">
		<xs:annotation>
			<xs:documentation>INFORMATION
The Qualifier ITEM id is unique for the system but are devided into groups to create a quailifier hierarchy)
Known Group are 1= Family member, 2 = trend, 3 = laterality, 4= Severity, 5= Numerical Value, 6= Causatiion, 7 = GMS 8=Certainty
The group ID may not be included in the message and the group and term attribute is populated for reference only</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="QualifierItemID" type="IntegerCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Values (look for the groups as well) Known Values are  1= Mother, 2=Father 3=Sister, 4= Brother, 5=Aunt, 6=Uncle, 7=Mat grandmother 8,= Maternal Grandfather, 9= Paternal grandmother, 10= Pat Grandfather, 11 = Worse, 12= Same, 13= Better, 14= Left, 15= Right, 16= Bilateral, 17 = Mild, 18 = Moderate, 19 =Severe, 20 = Cause of Death, 21 = YEsm 22= No, 23= Uncertain, 24 = Negation. Other Values are user definable</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Group" type="IntegerCodeType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NumericValueType">
		<xs:annotation>
			<xs:documentation>Handles numeric values</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Operator" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Any operator  for the numerical result</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Value" type="xs:double">
				<xs:annotation>
					<xs:documentation>The numerical value of a result</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Units" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unit of measure</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MinRangeOperator" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If the normal range is e.g. less than 10 then this is '&lt;'</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MaximumRangeOperator" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If the normal range is e.g. >10 then this would be '>'</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumericMinimum" type="xs:double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lower end of the normal range</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumericMaximum" type="xs:double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Upper end of the normal range</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StatusType">
		<xs:complexContent>
			<xs:extension base="IdentType">
				<xs:sequence>
					<xs:element name="Code" type="IntegerCodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Status code e.g. records received etc</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StatusDate" type="xs:string" minOccurs="0"/>
					<xs:element name="PatientType" type="IntegerCodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Type of patient for this status 1= Emergency 2= Immediate, 3= Provate 4 = Regular, 5= temporary resident  + user defined</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ProblemType">
		<xs:annotation>
			<xs:documentation>Contains problem details of a clinical problem or community episode
Problems may be linked in a heirarchy</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ProblemStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>0 = Past Problem 1 = Active Problem 2 = Health Admin</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:byte">
						<xs:enumeration value="0"/>
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GroupingStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>0 = No group 1 = Problem is grouped 2 = Problem is unified</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:byte">
						<xs:enumeration value="0"/>
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="EndDate" type="xs:string" minOccurs="0"/>
			<xs:element name="EndDatePart" type="xs:byte" minOccurs="0"/>
			<xs:element name="ProblemType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>1 = Clinical 2 = Care Episode 3 = Health Admin</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:byte">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Significance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>1 = Significant Problem 2 = Minor Problem 3 = health admin</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:byte">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ExpectedDuration" type="xs:integer" minOccurs="0"/>
			<xs:element name="ParentProblem" type="IdentType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EventType">
		<xs:complexContent>
			<xs:extension base="CodedItemBaseType">
				<xs:sequence>
					<xs:element name="GroupID" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Method of grouping linked events together such as BPs where there is no investigation test header, all members of the group have the same ID</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="Episodicity" minOccurs="0"/>
					<xs:element name="NumericValue" type="NumericValueType" minOccurs="0"/>
					<xs:element name="TextValue" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Used for a text qualifier by path labs when the read code is bad</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Abnormal" type="xs:integer" minOccurs="0"/>
					<xs:element name="GMS" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Whether recorded under GMS or not</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EventType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Internal use 0 = text, 1= Observation; 2= Problems; 5=Values; 7=Attachments; 8=Referrals; 10=Alerts; 11=Allergies; 12=Family History; 13=Immunisations</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:enumeration value="1"/>
								<xs:enumeration value="2"/>
								<xs:enumeration value="3"/>
								<xs:enumeration value="4"/>
								<xs:enumeration value="5"/>
								<xs:enumeration value="6"/>
								<xs:enumeration value="7"/>
								<xs:enumeration value="8"/>
								<xs:enumeration value="9"/>
								<xs:enumeration value="10"/>
								<xs:enumeration value="11"/>
