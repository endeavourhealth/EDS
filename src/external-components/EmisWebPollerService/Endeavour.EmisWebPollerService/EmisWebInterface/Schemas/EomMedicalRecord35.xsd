					<xs:element name="CollectionStartDate" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Date-time the collection was started</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CollectionEndDate" type="xs:string" minOccurs="0"/>
					<xs:element name="ReceivedByLabDateTime" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Date-time specimen received by the lab</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EDIInvestigationList" type="PathologyInvestigationListType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="InvestigationTypeBase">
		<xs:annotation>
			<xs:documentation>Common elements for "manual" and "EDI/Patholoy" Investigation entries</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="IdentType">
				<xs:sequence>
					<xs:element name="DisplayTerm" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Rubric Specified By LAB</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DataSource" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Type of data source. All 0 at this stage</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Code" type="StringCodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Read Code as specified by LAB</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TermID" type="StringCodeType" minOccurs="0"/>
					<xs:element name="Abnormal" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>0=Normal Tests 1=Abnormal Tests</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DataType">
						<xs:annotation>
							<xs:documentation>MAN = Manually filed, LAB=Filed by Lab Links</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ReportID" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>To be populated when used with PathologyInvestigationType</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpecimenID" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>To be populated when used with PathologyInvestigationType</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpecimenType" type="xs:string" minOccurs="0"/>
					<xs:element name="NumberOfTests" type="xs:integer" minOccurs="0"/>
					<xs:element name="RepInvNumber" type="xs:integer" minOccurs="0"/>
					<xs:element name="UserComment" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Comment Applied by User When Filing</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="InvestigationListType">
		<xs:sequence>
			<xs:element name="Investigation" type="InvestigationType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InvestigationType">
		<xs:complexContent>
			<xs:extension base="InvestigationTypeBase">
				<xs:sequence>
					<xs:element name="AssignedDate" type="xs:string"/>
					<xs:element name="DatePart" type="xs:short" minOccurs="0"/>
					<xs:element name="AssignedTime" type="xs:string" minOccurs="0"/>
					<xs:element name="DescriptiveText" type="xs:string" minOccurs="0"/>
					<xs:element name="OriginalAuthor" type="AuthorType" minOccurs="0"/>
					<xs:element name="AuthorID" type="IdentType" minOccurs="0"/>
					<xs:element name="PolicyID" type="xs:integer" minOccurs="0"/>
					<xs:element name="EventList" type="EventListType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PathologyInvestigationListType">
		<xs:sequence>
			<xs:element name="EDIInvestigation" type="PathologyInvestigationType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PathologyInvestigationType">
		<xs:complexContent>
			<xs:extension base="InvestigationTypeBase">
				<xs:sequence>
					<xs:element name="UserCommentAppliedBy" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>User Name/Mnenonic of user that applied the comment</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LabSpecifiedCommentList" type="EDICommentListType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Comments As specified by the LAB</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EDIResultStatus" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>FR - FinalComplete, IN interim, NA - not available</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InvestigationPerformedDateTime" type="xs:string" minOccurs="0"/>
					<xs:element name="FiledCode" type="StringCodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Read 2 Code of the Code that was Filed into the MR</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InvContainsStandAloneTest" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>0 - Battery Header / 1 Standalone Test - Fake Investigation Needed BUT not shown on display.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PathologyEventList" type="PathologyTestListType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PathologyTestListType">
		<xs:sequence>
			<xs:element name="PathologyEvent" type="PathologyTestType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PathologyTestType">
		<xs:complexContent>
			<xs:extension base="IdentType">
				<xs:sequence>
					<xs:element name="ReportID" type="xs:string"/>
					<xs:element name="SpecimenID" type="xs:string"/>
					<xs:element name="DisplayTerm" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Rubric as specified by the LAB</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Code" type="StringCodeType" minOccurs="0"/>
					<xs:element name="TermID" type="StringCodeType" minOccurs="0"/>
					<xs:element name="NumericValue" type="NumericValueType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Only Populate the Operator, Value and Units. The "RANGE" information is stored in the RangeInformationList</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TextValue" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Textual Result value - ie "POSTITIVE", "NEGATIVE".</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AbnormalIndicator" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Original AbnormalIndicator, as specified in the EDI - Ie HI, LO, MN etc.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EDIResultStatus" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>FR - FinalComplete, IN interim, NA - not available</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LabSpecifiedCommentList" type="EDICommentListType" minOccurs="0"/>
					<xs:element name="RangeInformationList" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Implied EDIFACT range standard.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="RangeInformation" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>For Min-Max, use both. For "Greater than" use MinRange and leave Max EMPTY. For "less than" use MaxRange and leave MinRange empty.</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="MinRange" type="xs:string" minOccurs="0"/>
											<xs:element name="MaxRange" type="xs:string" minOccurs="0"/>
											<xs:element name="RangeFTXList" type="EDICommentListType" minOccurs="0"/>
											<xs:element name="RangeQualifier" type="xs:string" minOccurs="0"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="FiledCode" type="StringCodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Read 2 Code of the Code that was Filed into the MR</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TestNumber" type="xs:integer" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EDICommentListType">
		<xs:sequence>
			<xs:element name="EDIComment" type="EDIComment" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EDIComment">
		<xs:sequence>
			<xs:element name="FTX" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
